spring:
  datasource:
    url: jdbc:mysql://localhost:3306/kakao_auth_lab?serverTimezone=Asia/Seoul&characterEncoding=UTF-8
    username: root
    password: root

  security:
    oauth2:
      client:
        registration:
          kakao:
            client-id: "여기에_카카오_REST_API_KEY"
            client-secret: "여기에_카카오_CLIENT_SECRET(사용중이면)"
            client-authentication-method: client_secret_post
            authorization-grant-type: authorization_code
            redirect-uri: "{baseUrl}/login/oauth2/code/{registrationId}"
            scope:
              - profile_nickname
              - account_email
              - profile_image
            client-name: Kakao

app:
  auth:
    jwt:
      secret: "여기에_아주_긴_랜덤문자열_최소32바이트_권장" # “서명용 비밀키”